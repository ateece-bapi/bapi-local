<h2 style="font-family: acumin-pro-extra-condensed; text-align: center; text-shadow: 2px 2px rgba(255, 200, 69, 0.3);">About Us</h2>
[/vc_column_text][vc_single_image image="404685" img_size="large" alignment="center"][vc_column_text]
<p style="text-align: center; font-size: 18px; font-family: 'acumin', sans-serif;"><strong>Founded in 1993, BAPI provides sensor solutions for customers around the globe. The innovation, manufacturing, and engineering all originate from our corporate office in Gays Mills, Wisconsin, USA.</strong></p>
[/vc_column_text][vc_btn title="Learn More About BAPI" style="custom" custom_background="#0057b8" custom_text="#e3e7ea" shape="square" align="center" i_icon_fontawesome="fas fa-arrow-alt-circle-right" css_animation="fadeIn" add_icon="true" link="url:https%3A%2F%2Fwww.bapihvac.com%2Fabout-us%2F||target:%20_blank|"][/vc_column][vc_column width="1/2" offset="vc_hidden-xs" css=".vc_custom_1651516773253{margin-left: 200px !important;}"][vc_raw_html][/vc_raw_html][/vc_column][/vc_row][vc_section full_width="stretch_row" content_placement="top" css=".vc_custom_1663178083993{margin-top: -20px !important;background-color: #d8d8d8 !important;}"][vc_row css=".vc_custom_1663174312634{background-color: #d8d8d8 !important;}"][vc_column][vc_column_text]
